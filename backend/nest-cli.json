{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/apis/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/apis/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/apis",
  "projects": {
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "apis": {
      "type": "application",
      "root": "apps/apis",
      "entryFile": "main",
      "sourceRoot": "apps/apis/src",
      "compilerOptions": {
        "tsConfigPath": "apps/apis/tsconfig.app.json"
      }
    },
    "keeper": {
      "type": "application",
      "root": "apps/keeper",
      "entryFile": "main",
      "sourceRoot": "apps/keeper/src",
      "compilerOptions": {
        "tsConfigPath": "apps/keeper/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}